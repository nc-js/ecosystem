{
	"name": "node-parcel-js-esm",
	"version": "1.0.0",
	"private": true,
	"description": "Node, JavaScript (ESM), Parcel",
	"keywords": [
		"esm",
		"javascript",
		"node",
		"parcel"
	],
	"source": "src/index.js",
	"files": [
		"dist"
	],
	"type": "module",
	"module": "dist/module.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/module.js"
		}
	},
	"scripts": {
		"build": "parcel build --cache-dir ./.parcel-cache && pnpm run dts",
		"dev": "parcel watch --cache-dir ./.parcel-cache && pnpm run dts",
		"dts": "tsc",
		"prepublint": "pnpm run build",
		"publint": "publint ."
	},
	"devDependencies": {
		"parcel": "catalog:bundler",
		"typescript": "catalog:bundler"
	},
	"targets": {
		"types": false
	}
}
