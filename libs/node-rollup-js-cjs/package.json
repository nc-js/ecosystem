{
	"name": "node-rollup-js-cjs",
	"version": "1.0.0",
	"private": true,
	"description": "Node, JavaScript (CommonJS), Rollup",
	"keywords": [
		"commonjs",
		"javascript",
		"node",
		"rollup"
	],
	"files": [
		"dist"
	],
	"type": "commonjs",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "rollup --config rollup.config.js",
		"dev": "pnpm run build -- -w",
		"prepublint": "pnpm run build",
		"publint": "publint ."
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "catalog:bundler",
		"@rollup/plugin-node-resolve": "catalog:bundler",
		"rollup": "catalog:bundler"
	}
}
