{
	"name": "node-rollup-ts-esm",
	"version": "1.0.0",
	"private": true,
	"description": "Node, TypeScript (ESM), Rollup",
	"keywords": [
		"esm",
		"node",
		"rollup",
		"typescript"
	],
	"files": [
		"dist"
	],
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "rollup --config rollup.config.ts --configPlugin typescript",
		"dev": "pnpm run build -- -w",
		"prepublint": "pnpm run build",
		"publint": "publint ."
	},
	"devDependencies": {
		"@rollup/plugin-typescript": "catalog:bundler",
		"rollup": "catalog:bundler",
		"rollup-plugin-dts": "catalog:bundler",
		"tslib": "catalog:bundler",
		"typescript": "catalog:bundler"
	}
}
