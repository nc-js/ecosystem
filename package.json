{
	"name": "@neoncitylights/ecosystem",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"libs/*"
	],
	"type": "module",
	"scripts": {
		"build": "pnpm run --r --if-present build",
		"fmt": "oxfmt",
		"fmt-ci": "oxfmt --check",
		"lint": "oxlint --type-aware --type-check",
		"fix": "oxlint --fix",
		"publint": "pnpm run --r --if-present publint",
		"inspect": "node-modules-inspector"
	},
	"devDependencies": {
		"node-modules-inspector": "catalog:",
		"oxfmt": "catalog:voidzero",
		"oxlint": "catalog:voidzero",
		"oxlint-tsgolint": "catalog:voidzero",
		"publint": "catalog:"
	}
}
